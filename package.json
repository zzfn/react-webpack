{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "react脚手架",
  "main": "src/index.tsx",
  "scripts": {
    "start:dev": "cross-env APP_ENV=development webpack server --config build/webpack.dev.js",
    "start:test": "cross-env APP_ENV=test webpack server --config build/webpack.dev.js",
    "start:pre": "cross-env APP_ENV=pre webpack server --config build/webpack.dev.js",
    "start:prod": "cross-env APP_ENV=production webpack server --config build/webpack.dev.js",
    "build:dev": "cross-env APP_ENV=development webpack --config build/webpack.prod.js",
    "build:test": "cross-env APP_ENV=test webpack --config build/webpack.prod.js",
    "build:pre": "cross-env APP_ENV=pre webpack --config build/webpack.prod.js",
    "build:prod": "cross-env APP_ENV=production webpack --config build/webpack.prod.js",
    "test": "eslint",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzfn/react-webpack.git"
  },
  "keywords": [
    "react"
  ],
  "author": "zzfn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zzfn/react-webpack/issues"
  },
  "homepage": "https://github.com/zzfn/react-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime-corejs3": "^7.14.9",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
