{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "react脚手架",
  "main": "src/index.tsx",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development APP_ENV=development webpack server --config build/webpack.dev.js",
    "start:test": "cross-env NODE_ENV=development APP_ENV=test webpack server --config build/webpack.dev.js",
    "start:pre": "cross-env NODE_ENV=development APP_ENV=pre webpack server --config build/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=development APP_ENV=production webpack server --config build/webpack.dev.js",
    "build:dev": "cross-env NODE_ENV=production APP_ENV=development webpack --config build/webpack.prod.js",
    "build:test": "cross-env NODE_ENV=production APP_ENV=test webpack --config build/webpack.prod.js",
    "build:pre": "cross-env NODE_ENV=production APP_ENV=pre webpack --co nfig build/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production APP_ENV=production webpack --config build/webpack.prod.js",
    "lint": "eslint . --ext .tsx,.ts,.js src",
    "lint:fix": "eslint . --ext .tsx,.ts,.js --fix src",
    "prettier": "prettier --write .",
    "lint:style": "stylelint **/*.scss",
    "lint:style-fix": "stylelint **/*.scss --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzfn/react-webpack.git"
  },
  "keywords": [
    "react"
  ],
  "author": "zzfn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zzfn/react-webpack/issues"
  },
  "homepage": "https://github.com/zzfn/react-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime-corejs3": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.4",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^7.0.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
